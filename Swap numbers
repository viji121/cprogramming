#include <stdio.h>

void swap(int num1, int num2);

int main() {
   int a = 5, b = 10;
   
   printf("Before swap: a = %d, b = %d\n", a, b);
   
   swap(a, b);
   
   printf("After swap: a = %d, b = %d\n", a, b);

   return 0;
}

void swap(int num1, int num2) {
   int temp;
   
   temp = num1;
   num1 = num2;
   num2 = temp;
}
